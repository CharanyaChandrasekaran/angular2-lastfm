{
  "name": "angular2-lastfm",
  "version": "1.0.0",
  "description": "A Last.fm API service for Angular2",
  "author": "mikeybyker+github@gmail.com",
  "keywords": [
    "angular2",
    "angular2js",
    "last.fm",
    "lastfm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mikeybyker/angular2-lastfm.git"
  },
  "scripts": {
    "start": "npm run tsc && npm run uglify",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "uglify": "uglifyjs dist/lastfm.service.js -o dist/lastfm.service.min.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "systemjs": "0.19.27",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "uglify-js": "^2.7.0"
  }
}
